{
  "flow_type": "template",
  "flow_name": "library_article",
  "status": "released",
  "rows": [
    {
      "name": "article_id",
      "value": "intro_goals",
      "exclude_from_translation": true
    },
    {
      "name": "data",
      "value": "article_data",
      "exclude_from_translation": true
    },
    {
      "name": "concat_article_id",
      "value": "@local.data.@local.article_id",
      "exclude_from_translation": true
    },
    {
      "name": "article_data",
      "value": "@data.@local.concat_article_id",
      "exclude_from_translation": true
    },
    {
      "name": "join",
      "value": "_",
      "exclude_from_translation": true
    },
    {
      "type": "begin_template",
      "name": "header",
      "value": "section_header",
      "action_list": "uncompleted | emit: uncompleted; "
    },
    {
      "name": "title",
      "value": "@local.article_data.title",
      "exclude_from_translation": true
    },
    {
      "type": "end_template"
    },
    {
      "type": "begin_template",
      "name": "intro_element",
      "value": "intro_element"
    },
    {
      "name": "text",
      "value": "@local.article_data.introduction"
    },
    {
      "name": "image",
      "value": "@local.article_data.introduction_image"
    },
    {
      "name": "text_2",
      "value": "@local.article_data.introduction_2"
    },
    {
      "type": "end_template"
    },
    {
      "type": "begin_display_group",
      "name": "dg_resources",
      "parameter_list": "style:column",
      "style_list": "justify-content: space-evenly"
    },
    {
      "type": "begin_items",
      "value": "@data.number",
      "condition": "@item.id =<4"
    },
    {
      "name": "resource_id_no",
      "value": "resource_id_@item.id"
    },
    {
      "name": "resource_data_no",
      "value": "resource_data_@item.id"
    },
    {
      "name": "concat_resource_id",
      "value": "@local.concat_article_id.@local.resource_id_no"
    },
    {
      "name": "concat_resource_data",
      "value": "@local.concat_article_id.@local.resource_data_no"
    },
    {
      "name": "resource_id",
      "value": "@data.@local.concat_resource_id"
    },
    {
      "name": "resource_data",
      "value": "@data.@local.concat_resource_data"
    },
    {
      "type": "begin_template",
      "name": "resource_element",
      "value": "resource_element",
      "condition": "@data.@local.concat_resource_id",
      "style_list": "margin-bottom: 12px"
    },
    {
      "name": "resource_type",
      "value": "@local.resource_data"
    },
    {
      "name": "resource_id",
      "value": "@local.resource_id"
    },
    {
      "type": "end_template"
    },
    {
      "type": "end_items"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "title",
      "name": "sections_title",
      "value": "@local.article_data.sections_title",
      "condition": "!!@local.article_data.sections_title"
    },
    {
      "type": "begin_items",
      "value": "@data.number",
      "condition": "@item.id =<5"
    },
    {
      "name": "section_text_no",
      "value": "section_text_@item.id"
    },
    {
      "name": "section_title_no",
      "value": "section_title_@item.id"
    },
    {
      "name": "concat_section_text",
      "value": "@local.concat_article_id.@local.section_text_no"
    },
    {
      "name": "concat_section_title",
      "value": "@local.concat_article_id.@local.section_title_no"
    },
    {
      "type": "begin_template",
      "name": "section_element",
      "value": "section_element",
      "condition": "@data.@local.concat_section_text"
    },
    {
      "name": "title",
      "value": "@data.@local.concat_section_title"
    },
    {
      "name": "text",
      "value": "@data.@local.concat_section_text"
    },
    {
      "type": "end_template"
    },
    {
      "type": "end_items"
    },
    {
      "type": "begin_items",
      "value": "@data.number",
      "condition": "@item.id =<5"
    },
    {
      "name": "message_title_no",
      "value": "message_title_@item.id"
    },
    {
      "name": "message_text_no",
      "value": "message_text_@item.id"
    },
    {
      "name": "message_guidance_no",
      "value": "message_guidance_@item.id"
    },
    {
      "name": "message_asset_no",
      "value": "message_asset_@item.id"
    },
    {
      "name": "concat_message_title",
      "value": "@local.concat_article_id.@local.message_title_no"
    },
    {
      "name": "concat_message_text",
      "value": "@local.concat_article_id.@local.message_text_no"
    },
    {
      "name": "concat_message_guidance",
      "value": "@local.concat_article_id.@local.message_guidance_no"
    },
    {
      "name": "concat_message_asset",
      "value": "@local.concat_article_id.@local.message_asset_no"
    },
    {
      "name": "message_title",
      "value": "@data.@local.concat_message_title"
    },
    {
      "name": "message_text",
      "value": "@data.@local.concat_message_text"
    },
    {
      "name": "message_guidance",
      "value": "@data.@local.concat_message_guidance"
    },
    {
      "name": "message_asset",
      "value": "@data.@local.concat_message_asset"
    },
    {
      "type": "begin_template",
      "name": "message_element",
      "value": "message_element",
      "condition": "@local.message_text"
    },
    {
      "name": "title",
      "value": "@local.message_title"
    },
    {
      "name": "has_disclaimer",
      "value": true,
      "condition": "@item.id == 1"
    },
    {
      "name": "message_text",
      "value": "@local.message_text"
    },
    {
      "name": "message_guidance",
      "value": "@local.message_guidance",
      "condition": "@local.message_guidance"
    },
    {
      "name": "asset",
      "value": "@local.message_asset"
    },
    {
      "type": "end_template"
    },
    {
      "type": "end_items"
    },
    {
      "type": "title",
      "name": "related_articles_title",
      "value": "@local.article_data.related_articles_title",
      "condition": "!!(@local.article_data.related_articles_title)"
    },
    {
      "type": "begin_items",
      "value": "@data.number"
    },
    {
      "name": "related_article_id_column",
      "value": "related_article_id_@item.id"
    },
    {
      "name": "concat_related_article_id_column",
      "value": "article_data.@local.article_id.@local.related_article_id_column"
    },
    {
      "name": "related_article_id",
      "value": "@data.@local.concat_related_article_id_column"
    },
    {
      "type": "begin_template",
      "name": "related_element",
      "value": "related_element",
      "condition": "@local.related_article_id"
    },
    {
      "name": "article_id",
      "value": "@local.related_article_id"
    },
    {
      "type": "end_template"
    },
    {
      "type": "end_items"
    },
    {
      "name": "task_list_title",
      "value": "@local.article_data.task_list_title"
    },
    {
      "type": "begin_display_group",
      "name": "dg_drawer",
      "parameter_list": "style:column",
      "condition": "@local.article_data.task_list_text"
    },
    {
      "type": "begin_drawer",
      "name": "drawer",
      "value": "@local.task_list_title",
      "parameter_list": "style: secondary"
    },
    {
      "type": "text",
      "name": "task_list_text",
      "value": "@local.article_data.task_list_text"
    },
    {
      "type": "end_drawer"
    },
    {
      "type": "end_display_group"
    }
  ]
}